{
    "root": true,
    "extends": ["prettier","walmart/configurations/es6-node"],
    "rules": {
        "quotes": ["warn", "single", { "allowTemplateLiterals": true }],
        "no-console":"off",
        "new-cap":"off",
        "no-magic-numbers":"off",
        "brace-style":"off",
        "filenames/match-regex":"off",
        "camelcase":"off",
        "max-nested-callbacks":["error", 10],
        "max-params":["error", 5],
        "max-statements":["error", 20]
    }
}
